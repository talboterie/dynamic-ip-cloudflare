{
    "name": "talboterie/dynamic-ip-cloudflare",
    "description": "Update Cloudflare DNS with Dynamic IP",
    "keywords": ["cloudflare", "ip", "dynamic"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Talbot",
            "email": "vincent@talbot.ninja"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "cloudflare/sdk": "^1.1",
        "laravel-zero/framework": "^8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["dynamic-ip-cloudflare"]
}
